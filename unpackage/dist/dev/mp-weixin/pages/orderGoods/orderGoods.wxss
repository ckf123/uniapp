@charset "UTF-8";
/**
 * 下方引入的为uView UI的集成样式文件，为scss预处理器，其中包含了一些"u-"开头的自定义变量
 * uView自定义的css类名和scss变量，均以"u-"开头，不会造成冲突，请放心使用 
 */
.f-s-28 {
  font-size: 28rpx;
  color: #999;
}
.Goods-wrap {
  background-color: #F5F5F5;
}
.notification {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translateX(-40px) translateY(-40px);
          transform: translateX(-40px) translateY(-40px);
}
.notification text {
  display: block;
  text-align: center;
  margin-top: 20rpx;
  color: #999;
}
.tba {
  position: -webkit-sticky;
  position: sticky;
  top: 0rpx;
  z-index: 1000;
}
.order-top {
  position: -webkit-sticky;
  position: sticky;
  top: 80rpx;
  z-index: 1000;
}
.order-top {
  background-color: #F5F5F5;
  width: 100%;
  height: 68rpx;
  line-height: 68rpx;
}
.order-top .client-color {
  width: 40%;
  text-align: left;
  margin-left: 10rpx;
}
.order-top .client-color .color-test-blue {
  color: blue;
  font-style: normal;
}
.u-body-item {
  width: 100%;
  min-width: 40%;
  font-size: 32rpx;
  color: #333;
}
.u-body-item .u-body-title-wrap {
  min-height: 182;
  flex: 1;
  overflow: hidden;
}
.u-body-item .u-body-title-wrap .u-body-item-title {
  margin-top: 6rpx;
}
.u-body-item image {
  width: 120rpx;
  flex: 0 0 120rpx;
  height: 120rpx;
  border-radius: 8rpx;
  margin: 0 24rpx;
}

